page{
  background: #edb31f;
}
.myRank{
  display: flex;
  flex-direction: row;
  justify-content:  space-between;
  align-content: center;
  align-items: center;
  margin: 10px;
}
.myRank-1{
   display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-content: center;
  align-items: center;
  margin: 5px;
  width: 100px;
}
.myRank-1 text{
  font-size: 15px;
  color: #666;
  padding-left:3px; 
  max-width: 60px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}
.avatar{
  width: 50px;
  height: 50px;
  border-radius: 50%;
  border:2px solid #e6e6e6;
}

.flow{
  width: 30px;
  height: 30px;
}

.rankTop{
  position: relative;
  height: 180px;
  width: 100%;
  background: #2c2c2c;
}

.jb-image-jin{
  height: 80px;
  width: 80px;
}
.jb-image-yin{
  height: 80px;
  width: 80px;
}
.jb-image-tong{
  height: 80px;
  width: 80px;
}

.rankTop-1{
  display: flex;
  flex-direction: column;
  position: absolute;
  top:5px;
  left: 50%;
  margin-left: -40px;
  align-content: center;
  align-items: center;
}
.rankTop-2{
  display: flex;
  flex-direction: column;
  position: absolute;
  top:10px;
  left: 50%;
  margin-left: -130px;
  align-content: center;
  align-items: center;
}

.rankTop-3{
   display: flex;
  flex-direction: column;
  position: absolute;
  top:15px;
  left: 50%;
  margin-left: 50px;
  align-content: center;
  align-items: center;
}

.rankTop-user{
   display: flex;
  flex-direction: column;
  justify-content:  center;
  align-content: center;
  align-items: center; 
}
.rankTop-user text{
  font-size: 13px;
  color: #666;
  max-width: 60px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap; 
}